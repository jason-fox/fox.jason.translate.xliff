<project basedir="." default="unit-test">
	<import file="../bootstrap.xml"/>
	<description>
		Expect that skeletons + XLIFF 1.0 with embedded elements can be used to create DITA
	</description>
	<target name="unit-test">
		<delete dir="${basedir}/out" failonerror="false"/>
		<delete dir="${basedir}/temp" failonerror="false"/>
		<exec-transtype transtype="xliff-dita" ditamap="translate.xlf" />
		<compare-output expectation="${basedir}/expected.dita" result="${basedir}/out/topic.dita"/>
	</target>
</project>